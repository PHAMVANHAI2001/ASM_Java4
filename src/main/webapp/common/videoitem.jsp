<div class="col tm-catalog-item">
	<div class="position-relative tm-thumbnail-container">
		<img src="images/${param.image}" alt="Image"
			class="img-fluid tm-catalog-item-img"> <a
			href="video-page" class="position-absolute tm-img-overlay">
			<i class="fad fa-play tm-overlay-icon"></i>
		</a>
	</div>
	<div class="p-4 bg-secondary tm-catalog-item-description bg-opacity-25">
		<h5 class="text-white mb-3 tm-catalog-item-title">${param.title}</h5>
		<p style="color: #AAAAAA; font-size: 16px;" class="m-0">
			<span class="views">${param.views} lượt xem</span> <span class="time">- ${param.dateupload}</span>
		</p>
	</div>
</div>